                  {
                    "type": 12,
                    "content": {
                      "version": "NotebookGroup/1.0",
                      "groupType": "editable",
                      "items": [
                        {
                          "type": 1,
                          "content": {
                            "json": "### All Logs\r\nAll *\"Request / Response\"* operational logs, with the exclusion of administration logs.",
                            "style": "upsell"
                          },
                          "name": "text - Monitor"
                        },
                        {
                          "type": 3,
                          "content": {
                            "version": "KqlItem/1.0",
                            "query": "AzureDiagnostics\r\n| where ResourceProvider == \"MICROSOFT.COGNITIVESERVICES\"\r\n| where Category == \"RequestResponse\"\r\n| extend properties = parse_json(properties_s)\r\n| extend APIOperationName = case(\r\nOperationName == \"Creates a completion for the provided prompt, parameters and chosen model.\", \"completions\",\r\nOperationName == \"Get a vector representation of a given input that can be easily consumed by machine learning models and algorithms.\", \"embeddings\",\r\nOperationName == \"Creates a completion for the chat message\", \"chat/completions\",\r\nOperationName == \"TBD1\", \"extensions/chat/completions\",\r\nOperationName == \"ImageGenerations_Create\", \"images/generations\",\r\nOperationName == \"TBD2\", \"images/generations:submit\",\r\nOperationName == \"TBD3\", \"audio/transactions\",\r\nOperationName == \"TBD4\", \"audio/translations\",\r\nOperationName\r\n)\r\n| where APIOperationName !has \"_List\"\r\n| where Resource in~ (split(\"{Resources:label}\", \", \"))\r\n| project\r\n    TimeGenerated,\r\n    Resource,\r\n    modelDeploymentName=properties.modelDeploymentName,\r\n    modelName=properties.modelName,\r\n    APIOperationName,\r\n    OperationName,\r\n    DurationMs,\r\n    CallerIPAddress,\r\n    //Location=location_s,\r\n    Location=Tenant_s,\r\n    apiName=properties.apiName,\r\n    modelVersion=properties.modelVersion,\r\n    //objectId=properties.objectId,\r\n    requestLength=properties.requestLength,\r\n    requestTime=properties.requestTime,\r\n    responseLength=properties.responseLength,\r\n    responseTime=properties.responseTime,\r\n    streamType=properties.streamType\r\n    | sort by TimeGenerated desc",
                            "size": 3,
                            "showAnalytics": true,
                            "title": "All Logs",
                            "timeContextFromParameter": "TimeRange",
                            "showRefreshButton": true,
                            "showExportToExcel": true,
                            "queryType": 0,
                            "resourceType": "microsoft.operationalinsights/workspaces",
                            "crossComponentResources": [
                              "{Workspace}"
                            ],
                            "visualization": "table",
                            "showExpandCollapseGrid": true,
                            "gridSettings": {
                              "rowLimit": 500,
                              "filter": true
                            }
                          },
                          "name": "query - insights - All Logs"
                        }
                      ]
                    },
                    "conditionalVisibility": {
                      "parameterName": "insightsTab",
                      "comparison": "isEqualTo",
                      "value": "insights-AllLogs"
                    },
                    "name": "group - Insights - All Logs"
                  }
                ]
